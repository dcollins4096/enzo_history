TOP_DIR = ..

include MAKE.CONFIG

#======================================================================
# Clean files
#======================================================================

.PHONY: clean
clean:
	-@rm -f *~

#======================================================================
# Clean files and reset configuration to default
#======================================================================

.PHONY: distclean
distclean: clean default
